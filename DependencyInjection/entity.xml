<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="SwagMigrationNext\Migration\Logging\SwagMigrationLoggingDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_logging"/>
        </service>

        <service id="SwagMigrationNext\Migration\Run\SwagMigrationRunDefinition"
                 class="SwagMigrationNext\Migration\Run\SwagMigrationRunDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_run"/>
        </service>
        <service id="SwagMigrationNext\Migration\Data\SwagMigrationDataDefinition"
                 class="SwagMigrationNext\Migration\Data\SwagMigrationDataDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_data"/>
        </service>

        <service id="SwagMigrationNext\Migration\Mapping\SwagMigrationMappingDefinition"
                 class="SwagMigrationNext\Migration\Mapping\SwagMigrationMappingDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_mapping"/>
        </service>

        <service id="SwagMigrationNext\Migration\Media\SwagMigrationMediaFileDefinition"
                 class="SwagMigrationNext\Migration\Media\SwagMigrationMediaFileDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_media_file"/>
        </service>

        <service id="SwagMigrationNext\Migration\Setting\GeneralSettingDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_general_setting"/>
        </service>

        <service id="SwagMigrationNext\Migration\Connection\SwagMigrationConnectionDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_connection"/>
        </service>
    </services>
</container>