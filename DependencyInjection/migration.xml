<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="SwagMigrationNext\Migration\Logging\LoggingService"
                 class="SwagMigrationNext\Migration\Logging\LoggingService">
            <argument type="service" id="swag_migration_logging.repository"/>
        </service>

        <service id="SwagMigrationNext\Migration\Asset\MediaFileService"
                 class="SwagMigrationNext\Migration\Asset\MediaFileService">
            <argument type="service" id="swag_migration_media_file.repository"/>
        </service>

        <service id="SwagMigrationNext\Migration\Converter\ConverterRegistry"
                 class="SwagMigrationNext\Migration\Converter\ConverterRegistry">
            <argument type="tagged" tag="shopware.migration.shopware55.converter"/>
        </service>

        <service id="SwagMigrationNext\Migration\Mapping\MappingService"
                 class="SwagMigrationNext\Migration\Mapping\MappingService">
            <argument type="service" id="swag_migration_mapping.repository"/>
            <argument type="service" id="locale.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="country.repository"/>
            <argument type="service" id="currency.repository"/>
        </service>

        <service id="SwagMigrationNext\Migration\Service\MigrationCollectService"
                 class="SwagMigrationNext\Migration\Service\MigrationCollectService"
                 public="true">
            <argument type="service" id="SwagMigrationNext\Migration\Profile\ProfileRegistry"/>
            <argument type="service" id="SwagMigrationNext\Migration\Gateway\GatewayFactoryRegistry"/>
            <argument type="service" id="SwagMigrationNext\Migration\Logging\LoggingService"/>
        </service>

        <service id="SwagMigrationNext\Migration\Service\MigrationWriteService"
                 class="SwagMigrationNext\Migration\Service\MigrationWriteService"
                 public="true">
            <argument type="service" id="swag_migration_data.repository"/>
            <argument type="service" id="SwagMigrationNext\Migration\Writer\WriterRegistry"/>
            <argument type="service" id="SwagMigrationNext\Migration\Asset\MediaFileService"/>
            <argument type="service" id="SwagMigrationNext\Migration\Logging\LoggingService"/>
        </service>

        <service id="SwagMigrationNext\Migration\Asset\CliAssetDownloadService"
                 class="SwagMigrationNext\Migration\Asset\CliAssetDownloadService"
                 public="true">
            <argument type="service" id="swag_migration_mapping.repository"/>
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="SwagMigrationNext\Migration\Asset\HttpAssetDownloadService"
                 class="SwagMigrationNext\Migration\Asset\HttpAssetDownloadService"
                 public="true">
            <argument type="service" id="swag_migration_media_file.repository"/>
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver"/>
            <argument type="service" id="SwagMigrationNext\Migration\Logging\LoggingService"/>
        </service>

        <service id="SwagMigrationNext\Migration\Service\MigrationEnvironmentService"
                 class="SwagMigrationNext\Migration\Service\MigrationEnvironmentService"
                 public="true">
            <argument type="service" id="SwagMigrationNext\Migration\Profile\ProfileRegistry"/>
            <argument type="service" id="SwagMigrationNext\Migration\Gateway\GatewayFactoryRegistry"/>
        </service>

        <service id="SwagMigrationNext\Migration\Service\MigrationProgressService"
                 class="SwagMigrationNext\Migration\Service\MigrationProgressService"
                 public="true">
            <argument type="service" id="swag_migration_run.repository"/>
            <argument type="service" id="swag_migration_data.repository"/>
            <argument type="service" id="swag_migration_media_file.repository"/>
        </service>

        <service id="SwagMigrationNext\Controller\MigrationController"
                 class="SwagMigrationNext\Controller\MigrationController"
                 public="true">
            <argument type="service" id="SwagMigrationNext\Migration\Service\MigrationCollectService"/>
            <argument type="service" id="SwagMigrationNext\Migration\Service\MigrationWriteService"/>
            <argument type="service" id="SwagMigrationNext\Migration\Asset\HttpAssetDownloadService"/>
            <argument type="service" id="SwagMigrationNext\Migration\Service\MigrationEnvironmentService"/>
            <argument type="service" id="swag_migration_profile.repository"/>
            <argument type="service" id="SwagMigrationNext\Migration\Service\MigrationProgressService"/>
        </service>

        <service id="SwagMigrationNext\Command\MigrationFetchDataCommand"
                 class="SwagMigrationNext\Command\MigrationFetchDataCommand">
            <argument type="service" id="SwagMigrationNext\Migration\Service\MigrationCollectService"/>
            <argument type="service" id="SwagMigrationNext\Migration\Service\MigrationEnvironmentService"/>
            <argument type="service" id="swag_migration_run.repository"/>
            <argument>migration:fetch:data</argument>
            <tag name="console.command" command="migration:fetch:data"/>
        </service>

        <service id="SwagMigrationNext\Command\MigrationWriteDataCommand"
                 class="SwagMigrationNext\Command\MigrationWriteDataCommand">
            <argument type="service" id="SwagMigrationNext\Migration\Service\MigrationWriteService"/>
            <argument>migration:write:data</argument>
            <tag name="console.command" command="migration:write:data"/>
        </service>

        <service id="SwagMigrationNext\Command\MigrationDownloadAssetsCommand"
                 class="SwagMigrationNext\Command\MigrationDownloadAssetsCommand">
            <argument type="service" id="swag_migration_mapping.repository"/>
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver"/>
            <argument type="service" id="event_dispatcher"/>
            <tag name="console.command" command="migration:assets:download"/>
            <tag name="kernel.event_subscriber"/>
        </service>
    </services>
</container>
