<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SwagMigrationNext\Migration\Data\SwagMigrationDataDefinition"
                 class="SwagMigrationNext\Migration\Data\SwagMigrationDataDefinition">
            <tag name="shopware.entity.definition" entity="swag_migration_data"/>
        </service>

        <service id="swag_migration_data.repository" class="Shopware\Core\Framework\ORM\EntityRepository" public="true">
            <argument>SwagMigrationNext\Migration\Data\SwagMigrationDataDefinition</argument>
            <argument type="service" id="Shopware\Core\Framework\ORM\Read\EntityReaderInterface"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\VersionManager"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\EntitySearcherInterface"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\EntityAggregatorInterface"/>
            <argument type="service" id="event_dispatcher"/>
        </service>

        <service id="SwagMigrationNext\Migration\MigrationCollectService"
                 class="SwagMigrationNext\Migration\MigrationCollectService"
                 public="true">
            <argument type="service" id="SwagMigrationNext\Profile\ProfileRegistry"/>
            <argument type="service" id="SwagMigrationNext\Gateway\GatewayFactoryRegistry"/>
            <argument type="service" id="swag_migration_data.repository"/>
        </service>

        <service id="SwagMigrationNext\Migration\MigrationWriteService"
                 class="SwagMigrationNext\Migration\MigrationWriteService"
                 public="true">
            <argument type="service" id="swag_migration_data.repository"/>
            <argument type="service" id="SwagMigrationNext\Migration\Writer\WriterRegistry"/>
        </service>

        <service id="SwagMigrationNext\Migration\MigrationValidateService"
                 class="SwagMigrationNext\Migration\MigrationValidateService"
                 public="true">
            <argument type="service" id="swag_migration_data.repository"/>
            <argument type="service" id="SwagMigrationNext\Migration\Validator\ValidatorRegistry"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\ProductWriter"
                 class="SwagMigrationNext\Migration\Writer\ProductWriter">
            <tag name="shopware.migration.writer"/>
            <argument type="service" id="product.repository" />
        </service>

        <service id="SwagMigrationNext\Migration\Controller\MigrationController"
                 class="SwagMigrationNext\Migration\Controller\MigrationController"
                 public="true">
            <argument type="service" id="SwagMigrationNext\Migration\MigrationCollectService"/>
            <argument type="service" id="SwagMigrationNext\Migration\MigrationWriteService"/>
        </service>
    </services>
</container>
