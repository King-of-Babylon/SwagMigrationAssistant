<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SwagMigrationNext\Gateway\GatewayService" class="SwagMigrationNext\Gateway\GatewayService">
            <argument type="tagged" tag="shopware.migration.gateway_factory"/>
        </service>

        <service id="SwagMigrationNext\Gateway\Shopware5\Api\Factory" class="SwagMigrationNext\Gateway\Shopware5\Api\Factory">
            <tag name="shopware.migration.gateway_factory"/>
        </service>

        <service id="SwagMigrationNext\Gateway\Shopware5\Local\Factory" class="SwagMigrationNext\Gateway\Shopware5\Local\Factory">
            <tag name="shopware.migration.gateway_factory"/>
        </service>
    </services>
</container>
