<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SwagMigrationNext\Migration\Validator\ValidatorRegistry" class="SwagMigrationNext\Migration\Validator\ValidatorRegistry">
            <argument type="tagged" tag="shopware.migration.validator"/>
        </service>

        <service id="SwagMigrationNext\Migration\Validator\ProductValidator"
                 class="SwagMigrationNext\Migration\Validator\ProductValidator">
            <tag name="shopware.migration.validator"/>
            <argument type="service" id="product.repository"/>
        </service>
    </services>
</container>
