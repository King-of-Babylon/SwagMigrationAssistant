<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SwagMigrationNext\Migration\Writer\WriterRegistry"
                 class="SwagMigrationNext\Migration\Writer\WriterRegistry">
            <argument type="tagged" tag="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\ProductWriter"
                 class="SwagMigrationNext\Migration\Writer\ProductWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\TranslationWriter"
                 class="SwagMigrationNext\Migration\Writer\TranslationWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\CategoryWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\MediaWriter"
                 class="SwagMigrationNext\Migration\Writer\MediaWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\NumberRangeWriter">
            <argument type="service" id="number_range.repository" />
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\CustomerAttributeWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\CustomerWriter"
                 class="SwagMigrationNext\Migration\Writer\CustomerWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\OrderAttributeWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\OrderWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <argument type="service" id="Shopware\Core\Framework\Struct\Serializer\StructNormalizer"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\CustomerGroupAttributeWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\CustomerGroupWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\ManufacturerAttributeWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\ProductAttributeWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\ProductPriceAttributeWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\CategoryAttributeWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>

        <service id="SwagMigrationNext\Migration\Writer\PropertyGroupOptionWriter">
            <argument type="service" id="Shopware\Core\Framework\DataAbstractionLayer\Write\EntityWriter"/>
            <tag name="shopware.migration.writer"/>
        </service>
    </services>
</container>
