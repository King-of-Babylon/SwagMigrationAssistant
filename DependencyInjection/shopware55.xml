<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Shopware 5.5 Profile -->
        <service id="SwagMigrationAssistant\Profile\Shopware55\Shopware55Profile">
            <tag name="shopware.migration.profile"/>
        </service>

        <!-- Shopware 5.5 Profile Converter -->
        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter" abstract="true">
            <argument id="SwagMigrationAssistant\Migration\Mapping\MappingService" type="service"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\ProductAttributeConverter"
                 parent="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter">
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\ProductPriceAttributeConverter"
                 parent="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter">
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\ManufacturerAttributeConverter"
                 parent="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter">
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\CustomerAttributeConverter"
                 parent="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter">
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\OrderAttributeConverter"
                 parent="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter">
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\ProductConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Media\MediaFileService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\TranslationConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\CategoryAttributeConverter"
                 parent="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter">
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\CategoryConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Media\MediaFileService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\MediaFolderConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\MediaConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Media\MediaFileService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\CustomerConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\OrderConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="Shopware\Core\Checkout\Cart\Tax\TaxCalculator"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\OrderDocumentConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Media\MediaFileService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\CustomerGroupAttributeConverter"
                 parent="SwagMigrationAssistant\Profile\Shopware55\Converter\AttributeConverter">
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\CustomerGroupConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\PropertyGroupOptionConverter">
            <argument id="SwagMigrationAssistant\Migration\Mapping\MappingService" type="service"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Media\MediaFileService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\NumberRangeConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService" />
            <argument type="service" id="number_range_type.repository" />
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\CurrencyConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\LanguageConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\SalesChannelConverter">
            <argument type="service" id="SwagMigrationAssistant\Migration\Mapping\MappingService"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <argument type="service" id="payment_method.repository"/>
            <argument type="service" id="shipping_method.repository"/>
            <argument type="service" id="country.repository"/>
            <tag name="shopware.migration.converter"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware55\Converter\NewsletterRecipientConverter">
            <argument id="SwagMigrationAssistant\Migration\Mapping\MappingService" type="service"/>
            <argument type="service" id="SwagMigrationAssistant\Migration\Logging\LoggingService"/>
            <tag name="shopware.migration.converter"/>
        </service>
    </services>
</container>
