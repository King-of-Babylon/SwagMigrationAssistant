{% block swag_migration_wizard_page_2 %}
<div class="swag-migration-wizard swag-migration-wizard-page-2">
    {% block swag_migration_wizard_page_2_content %}
    <div class="swag-migration-wizard__content">
        {% block swag_migration_wizard_page_2_information %}
        <div class="content__information">
            {% block swag_migration_wizard_page_2_title %}
            <h3 class="information__header">
                {{ $tc('swag-migration.wizard.pages.2.contentTitle') }}
            </h3>
            {% endblock %}

            {% block swag_migration_wizard_page_2_svg %}
            <swag-svg-step2></swag-svg-step2>
            {% endblock %}

            {% block swag_migration_wizard_page_2_api_hint %}
            {{ $tc('swag-migration.wizard.pages.2.contentInformation') }}
            {% endblock %}
        </div>
        {% endblock %}
    </div>
    {% endblock %}

    {% block swag_migration_wizard_page_2_breadcrumb %}
    <div class="swag-migration-wizard__breadcrumb">
        {% block swag_migration_wizard_page_2_breadcrumb_content %}
        {% block swag_migration_wizard_page_2_breadcrumb_icon %}
        <sw-icon name="default-badge-info" size="14"></sw-icon>
        {% endblock %}

        {% block swag_migration_wizard_page_2_breadcrumb_path %}
        <swag-breadcrumb :description="breadcrumbDescription"
                         :items="breadcrumbItems">
        </swag-breadcrumb>
        {% endblock %}
        {% endblock %}
    </div>
    {% endblock %}
</div>
{% endblock %}