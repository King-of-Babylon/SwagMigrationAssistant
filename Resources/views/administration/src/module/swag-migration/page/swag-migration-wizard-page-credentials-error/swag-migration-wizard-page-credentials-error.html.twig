{% block swag_migration_wizard_page_credentials_error %}
    <div class="swag-migration-wizard swag-migration-wizard-page-credentials-error">
        {% block swag_migration_wizard_page_credentials_error_content %}
            <div class="swag-migration-wizard__content">
                {% block swag_migration_wizard_page_credentials_error_information %}
                    <div class="content__information">
                        {% block swag_migration_wizard_page_credentials_error_title %}
                            <h3 class="information__header">
                                {{ $tc('swag-migration.wizard.pages.credentials.error.contentTitle') }}
                            </h3>
                        {% endblock %}

                        {% block swag_migration_wizard_page_credentials_error_svg %}
                            <swag-svg-wizard-credentials-error></swag-svg-wizard-credentials-error>
                        {% endblock %}

                        {% block swag_migration_wizard_page_credentials_error_api_hint %}
                            {{ $tc('swag-migration.wizard.pages.credentials.error.contentInformation') }}
                        {% endblock %}
                    </div>
                {% endblock %}
            </div>
        {% endblock %}

        {% block swag_migration_wizard_page_credentials_error_alert %}
            <sw-alert variant="warning" id="alert-error-message">
                <p v-html=$tc('swag-migration.wizard.pages.credentials.error.alertErrorTitle')"></p>
                {{ errorMessage }}
            </sw-alert>
        {% endblock %}
    </div>
{% endblock %}